<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E2EEF8"
    android:backgroundTint="#004F7CAC"
    android:backgroundTintMode="src_atop"
    tools:context=".homepage">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="651dp"
        android:layout_height="327dp"
        android:src="@drawable/hf4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="400dp"
        android:layout_height="151dp"
        android:fontFamily="@font/anonymous_pro_bold"
        android:shadowColor="#3092FB"
        android:shadowDx="0"
        android:shadowDy="5"
        android:shadowRadius="2"
        android:text="@string/description"
        android:textAlignment="center"
        android:textColor="#7DB1EA"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.477" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/birb"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="327dp" />

</androidx.constraintlayout.widget.ConstraintLayout>